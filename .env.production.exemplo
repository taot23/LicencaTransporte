# Arquivo .env para o servidor Google Cloud
# Copie este arquivo para .env e preencha os valores reais

# Database Configuration
DATABASE_URL=postgresql://usuario:senha@host:5432/database_name
PGDATABASE=aet_database
PGHOST=localhost
PGPORT=5432
PGUSER=aet_user
PGPASSWORD=senha_segura

# Environment
NODE_ENV=production

# Session Configuration (IMPORTANTE para autenticação)
SESSION_SECRET=chave_super_secreta_para_sessoes_muito_longa_e_aleatoria_123456789

# Upload Directory
UPLOAD_DIR=/var/www/aetlicensesystem/uploads

# Debug Options (para diagnosticar problemas de permissões)
DEBUG_PERMISSIONS=true
DEBUG_AUTH=true

# API Configuration (opcional)
API_BASE_URL=https://nvslicencas.com.br

# Security
CORS_ORIGIN=https://seu-dominio.com

# Logs
LOG_LEVEL=info